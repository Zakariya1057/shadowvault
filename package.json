{
  "name": "shadowvault",
  "version": "7.5.8",
  "description": "Tool for managing env across multiple team members.",
  "main": "dist/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "shadowvault": "./dist/index.js"
  },
  "dependencies": {
    "inversify": "^6.0.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.4",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "test": "jest",
    "run": "npx ts-node src/index.ts",
    "build": "npx webpack --mode production",
    "test:cov": "jest --coverage --config jest.config.ts --no-cache",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "lint:check": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "prettier": "prettier --write '**/*.{ts,tsx}'",
    "precommit": "npm run lint:fix && npm run prettier && npm run test",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
